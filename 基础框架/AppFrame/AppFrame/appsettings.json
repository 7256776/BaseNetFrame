{
  "ConnectionStrings": {
    "Default": "Server=.; Database=AppDB; user id=sa;pwd=sa;"
  },
  "File": {
    "MultipartBodyLengthLimit": 5   //设置上传文件大小 单位/M
  },
  "App": {
    "ServerRootAddress": "http://localhost:20199/",
    "ClientRootAddress": "http://localhost:4200/",
    //设置允许跨域访问的url,多个url用','号分割,如需允许全部url设置 '*'  http://localhost:54403
    "CorsOrigins": "*"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug", //Warning
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*"
}
